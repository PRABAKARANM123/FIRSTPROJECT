package Object;

public class UseAc {
	public static void main(String[]args)
	{
		Ac a=new Ac();
		a.brand="STAR";
		a.colour="white";
		a.price=35000;
		a.taxAmount=5;
		a.taxPercent=5/100*35000;
		a.netAmount=a.price+a.taxPercent;
		System.out.println(a.brand);
		System.out.println(a.netAmount);
		
		
		
		
		
		
	}

}
