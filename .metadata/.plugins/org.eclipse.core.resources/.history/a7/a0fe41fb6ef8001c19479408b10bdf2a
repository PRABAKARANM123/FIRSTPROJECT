package Object;

public class NewPencile 
{
	public static void main(String[]args) 
	{
		
	
	Pencile p =new Pencile();
	p.brand="natraj";
	p.price=15;
	p.taxAmount=1.5f;
	p.discountAmount=1;
	p.netPrice=p.price+p.taxAmount-p.discountAmount;
	System.out.println(p.brand);
	System.out.println(p.netPrice);
	
	
	
			
	
	
	}
}
